
{
  "manifest_version": 3,
  "name": "LinkedIn AI Post Generator",
  "version": "1.0",
  
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": ["activeTab", "scripting"]
}

